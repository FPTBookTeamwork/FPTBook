@model IEnumerable<FPTBookDemo.Models.ApplicationUser>
@{
    ViewData["Title"] = "User List";
}
<h1>User List</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                UserName
            </th>
            <th></th>
        </tr>
    </thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.UserName
            </td>
            <td>
                <a asp-action="SetPassword" asp-route-id="@item.Id">SetPassword</a>
            </td>
        </tr>
    }
</tbody>
</table>
